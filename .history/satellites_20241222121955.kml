    <!-- Style for LEO satellites -->
    <Style id="leoStyle">
      <IconStyle>
        <scale>1.0</scale>
        <Icon>
          <href>http://maps.google.com/mapfiles/kml/pushpin/blue-pushpin.png</href>
        </Icon>
        <hotSpot x="20" y="2" xunits="pixels" yunits="pixels"/>
      </IconStyle>
    </Style>

    <!-- Style for MEO satellites -->
    <Style id="meoStyle">
